<div class='centerHeader'>
  <h2><%= titleCap(colorChoice) %>!</h2>
</div>
<ul>
  <% let i = 0; %> <!-- Keeps track for making rows -->
  <% colors.forEach((color) => { %>
    <% if (color.category===colorChoice) { %>
      <% if (i % 4 === 0 ) { %>
        </div><div class='someRow'>
      <% } %>
    <li>
      <div class="displaybox" style="background-color: <%= color.hex %>"></div>
      <%= color.keyword %>
      <%= color.hex %>
    </li>
    <%  i+= 1;} %>
  <% }) %>

</ul>
